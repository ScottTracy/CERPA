@model IEnumerable<CERPA.Models.ConfigurableAssemblyVariable>

@{
    ViewBag.Title = "AddAnother";
}

<h2>AddAnotherr</h2>

<p>
    @Html.ActionLink("Create New", "CreateAnother")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PartID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VariableName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISRequired)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PartID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VariableName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ISRequired)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
