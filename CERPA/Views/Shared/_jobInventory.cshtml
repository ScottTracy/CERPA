@model IEnumerable<CERPA.Models.PickOrder>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.PartId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PartQuantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.PartId)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.PartQuantity)
        </td>
    </tr>
}

</table>
